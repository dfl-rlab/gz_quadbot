    <!-- ========= 立體結構：上坡 + 平台 + 下坡 ========= -->

    <!-- 上坡（長10m、寬6m、厚0.5m；pitch≈0.15rad） -->
    <model name="up_ramp">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <box><size>10 16 0.3</size></box>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>1000</mu>
                <mu2>1000</mu2>
                <slip1>0.0</slip1>
                <slip2>0.0</slip2>
              </ode>
              <torsional>
                <ode>
                  <coefficient>5.0</coefficient>
                  <use_patch_radius>true</use_patch_radius>
                  <patch_radius>1.0</patch_radius>
                  <surface_radius>0.5</surface_radius>
                  <slip>0.0</slip>
                </ode>
              </torsional>
            </friction>
            <contact>
              <ode>
                <kp>1e6</kp>
                <kd>1e2</kd>
                <max_vel>0.05</max_vel>
                <min_depth>0.001</min_depth>
              </ode>
            </contact>
          </surface>
        </collision>
        <visual name="visual">
          <geometry>
            <box><size>10 16 0.3</size></box>
          </geometry>
          <material>
            <ambient>0.7 0.7 0.7 1</ambient>
            <diffuse>0.7 0.7 0.7 1</diffuse>
          </material>
        </visual>
      </link>
      <!-- pose: x y z roll pitch yaw -->
      <pose>-8 5 0.5 0.15 0 0</pose>
    </model>

